windows:
	gcc -O3 ../ray_tracer_threaded.c  ../utilities/*.c  ../utilities/geometric/*.c  -lm -o ../out/ray_tracer_threaded.exe  -Wall -pedantic -Wno-pedantic-ms-format -std=c11

linux:
	gcc -O3 ../ray_tracer_threaded.c  ../utilities/*.c  ../utilities/geometric/*.c  -lm -lpthread -o ../out/ray_tracer_threaded.out  -Wall -pedantic -std=c11
